{
  "name": "@neetly/eslint-config",
  "version": "0.7.0",
  "homepage": "https://github.com/neetly/eslint-config",
  "repository": {
    "type": "git",
    "url": "git@github.com:neetly/eslint-config.git"
  },
  "license": "MIT",
  "exports": {
    ".": "./src/index.js",
    "./node": "./src/node.js",
    "./web": "./src/web.js",
    "./react": "./src/react.js",
    "./experimental-project-references": "./src/experimental-project-references.js"
  },
  "files": [
    "./src"
  ],
  "scripts": {
    "lint": "eslint --max-warnings 0 . && prettier --check .",
    "lint:fix": "eslint --max-warnings 0 --fix . && prettier --write ."
  },
  "dependencies": {
    "@rushstack/eslint-patch": "1.7.2",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-unicorn": "51.0.1"
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "packageManager": "yarn@4.1.0"
}
